          "example": "2017-01-28T01:22:14.398Z"
        },
        "key": {
          "type": "string",
          "x-go-name": "Key",
          "example": "TAKwGc6Jr4i8Z487"
        },
        "totalCount": {
          "type": "integer",
          "format": "int64",
          "x-go-name": "TotalCount",
          "example": 2800
        }
      },
      "x-go-package": "github.com/fufuceng/getir-challange/records"
    }
  },
  "responses": {
    "inMemoryErrorResponse": {
      "description": "inMemoryErrorResponse",
      "schema": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "uint64",
