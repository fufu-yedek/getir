  "host": "fufu-getir-challange.herokuapp.com",
  "basePath": "/",
  "paths": {
    "/in-memory": {
      "get": {
        "description": "retrieve key-value pair",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http",
          "https"
        ],
        "tags": [
          "InMemory"
        ],
        "operationId": "Retrieve",
        "parameters": [
          {
            "type": "string",
            "example": "active-tabs",
            "x-go-name": "Key",
            "name": "key",
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/inMemoryResponse"
          },
          "400": {
            "$ref": "#/responses/inMemoryErrorResponse"
          },
          "500": {
            "$ref": "#/responses/inMemoryErrorResponse"
          }
        }
      },
      "post": {
        "description": "create or update key-value",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http",
          "https"
        ],
        "tags": [
          "InMemory"
        ],
        "operationId": "CreateOrUpdate",
        "parameters": [
          {
            "name": "Body",
            "in": "body",
            "schema": {
              "type": "object",
              "required": [
                "key",
                "value"
              ],
              "properties": {
                "key": {
                  "type": "string",
                  "x-go-name": "Key",
                  "example": "active-tabs"
                },
                "value": {
                  "type": "string",
                  "x-go-name": "Value",
                  "example": "getir"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/inMemoryResponse"
          },
          "400": {
            "$ref": "#/responses/inMemoryErrorResponse"
          },
          "500": {
            "$ref": "#/responses/inMemoryErrorResponse"
          }
        }
      }
    },
    "/records": {
      "post": {
        "description": "List \u0026 filter records",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http",
          "https"
        ],
        "tags": [
          "Records"
        ],
        "operationId": "List-Records",
        "parameters": [
          {
            "name": "Body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "end_date": {
                  "type": "string",
                  "format": "string",
                  "x-go-name": "EndDate",
                  "example": "2020-11-20"
                },
                "max_count": {
                  "type": "integer",
                  "format": "uint64",
                  "x-go-name": "MaxCount",
                  "example": 50
                },
                "min_count": {
                  "type": "integer",
                  "format": "uint64",
                  "x-go-name": "MinCount",
                  "example": 30
                },
                "start_date": {
                  "type": "string",
                  "format": "string",
                  "x-go-name": "StartDate",
                  "example": "2020-12-20"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/listRecordsSwaggerResponse"
          },
          "400": {
            "$ref": "#/responses/listRecordsSwaggerErrorResponse"
          },
          "500": {
            "$ref": "#/responses/listRecordsSwaggerErrorResponse"
          }
        }
      }
    }
  },
  "definitions": {
